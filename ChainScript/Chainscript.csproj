<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Stratumn.Chainscript</PackageId>
    <Title>Chainscript</Title>
    <Description>Official C# implementation of https://github.com/stratumn/chainscript</Description>
    <Version>0.0.11.0</Version>
    <Authors>Stratumn</Authors>
    <Company>Stratumn</Company>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/15095987</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/stratumn/csharp-chainscript/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/stratumn/csharp-chainscript</RepositoryUrl>
    <PackageTags>chainscript</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.11.4" />
    <PackageReference Include="JmesPath.Net" Version="1.0.125" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.8.6.7" />
    <PackageReference Include="Stratumn.CanonicalJson" Version="0.0.8" />
  </ItemGroup>

</Project>
